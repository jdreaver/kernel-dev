# Enable NFS filesystem
CONFIG_NFS_FS=y
CONFIG_NFS_V3=y
CONFIG_NFS_V4=y

# Disable some subsystems and modules we don't care about
CONFIG_BT=n # bluetooth
CONFIG_DRM=n
CONFIG_DVB_CORE=n
CONFIG_DVB_NET=n
CONFIG_INPUT_TOUCHSCREEN=n
CONFIG_MEDIA_SUPPORT=n
CONFIG_MEDIA_CONTROLLER=n
CONFIG_MEDIA_TUNER=n
CONFIG_MTD=n
CONFIG_NETFILTER=n
CONFIG_SOUND=n
CONFIG_VIDEO_DEV=n
CONFIG_WIRELESS=n
